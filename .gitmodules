[submodule "HOME/zsh/pure"]
	path = roles/dotfiles/files/.zsh/bundles/pure
	url = git@github.com:sindresorhus/pure.git
[submodule "HOME/zsh/async"]
	path = roles/dotfiles/files/.zsh/bundles/async
	url = git@github.com:mafredri/zsh-async.git
[submodule "HOME/zsh/zsh-completions"]
	path = roles/dotfiles/files/.zsh/bundles/zsh-completions
	url = git@github.com:zsh-users/zsh-completions.git
[submodule "HOME/zsh/zsh-syntax-highlighting"]
	path = roles/dotfiles/files/.zsh/bundles/zsh-syntax-highlighting
	url = git@github.com:zsh-users/zsh-syntax-highlighting.git
[submodule "vendor/ansible"]
	path = vendor/ansible
	url = git@github.com:ansible/ansible.git
[submodule "vendor/terminal-snazzy"]
	path = vendor/terminal-snazzy
	url = git@github.com:sindresorhus/terminal-snazzy.git
[submodule "vendor/virtualenv"]
	path = vendor/virtualenv
	url = git@github.com:pypa/virtualenv.git
[submodule "roles/fonts/files"]
	path = roles/fonts/files
	url = git@github.com:jamesramsay/operator-mono.git
